var newArray = Array.apply(null, Array(100)).map( function(val,index) {
  return index + 1;
)};


function request () {
  var evenArray = newArray.filter(result => result%2 == 0);
  
  return new Promise((resolve, reject) => {
    evenArray.subscribe(resolve, reject);
  }
}

function square() {
var squareArray = request.then(resulr => {
var temp = result.map(function(val) {
  return Math.pow(value,2);
}
}).catch(err => console.log("Error"));


var sumArray = squareArray.reduce((a,b) => a+b,0)

}

